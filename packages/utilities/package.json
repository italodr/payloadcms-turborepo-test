{
  "name": "utilities",
  "version": "1.0.0",
  "type": "module",
  "homepage:": "",
  "repository": "",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm clean && pnpm build:swc && pnpm build:types",
    "build:swc": "swc ./src -d ./dist --config-file .swcrc --strip-leading-paths",
    "build:types": "rm -f *.tsbuildinfo && tsc --emitDeclarationOnly --project tsconfig.types.json --outDir dist",
    "clean": "rm -rf dist",
    "lint": "eslint src",
    "lint:fix": "eslint --fix --ext .ts,.tsx src",
    "prepublishOnly": "pnpm clean && pnpm build"
  },
  "dependencies": {
    "@test/utilities": "latest"
  },
  "peerDependencies": {
    "react": "19.2.4",
    "react-dom": "19.2.4"
  }
}
